/**
 * TerminalTest
 * --
 * $Id$
 * $timestamp: Sat Mar 16 18:10:18 1996 by Matthias L. Jugel :$
 */

import java.applet.Applet;
import java.awt.BorderLayout;

import display.Terminal;

public class TerminalTest extends Applet
{
  Terminal display = new Terminal();

  public void init()
  {
     setLayout(new BorderLayout());
     add("Center", display);
  }
  
  public void start()
  {
    for(int l = 0; l < 24; l++)
      display.putString(l+" ");
    for(int l = 1; l < 23; l++)
      display.putString("["+l+"] **++** @@@@@@@@@@@@ ------------------------\n");
  }
}
@
